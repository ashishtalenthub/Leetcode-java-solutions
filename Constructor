class Demo{
    int id;
    String name;
    
    Demo(int i,String n)// parametrised constructor
    {
    id = i;
    name = n;
    }
    void display(){System.out.println(id+" "+name);}
 
    public static void main(String args[]){
    Demo s1 = new Demo(111,"ashish");
    Demo s2 = new Demo(222,"kumar");
    s1.display();
    s2.display();
   }
}

